/*TITL*/
#include<bits\stdc++.h>
using namespace std;
const int MAX=1e3;
int line[MAX][MAX],match[MAX],used[MAX];
int n,m;
bool find(int x)
{
    for(int i=1;i<=m;i++)
    {
        if(line[x][i]&&!used[i])
        {
         used[i]=1;
         if(find(i)||!match[i])
         {
             match[i]=x;
             return true;
         }
        }
    }
    return false;
}
int hungarian()
{
    int sum=0;
    for(int i=1;i<=n;i++)
    {
        memset(used,0,sizeof(used));
        if(find(i))
            sum++;
    }
    return sum;
}
